<% if @users.nil?%>
Be the first!
<%else%>
<div class="row">
<% @users.users_joined.each do |u| %>
<div class="col-md-6">
<%= link_to image_tag(u.user.useravatar.url(:small), :size => "42x42", :class => "img-thumbnail" ), user_path(u.user.id)%> 
<%= link_to u.user.full_name, user_path(u.user.id)%>
</div>
<% end %>
</div>
<%end%>
