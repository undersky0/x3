<% if @users.nil?%>

<%else%>
<div class="row">
<% @users.users_joined.each do |u| %>
<div class="col-md-6 bl">
<%= link_to image_tag(u.user.useravatar.url(:tiny)), user_path(u.user.id), :class => "img-thumbnail", :style=> "width:42px; height:42px;" %> 
<%= link_to u.user.full_name, user_path(u.user.id)%>
</div>
<% end %>
</div>
<%end%>
